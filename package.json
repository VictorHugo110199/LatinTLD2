{
  "name": "template-remix-tailwind-sass",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "build": "remix build",
    "dev": "concurrently \"npm run sass\" \"remix dev --manual\" ",
    "start": "remix-serve build",
    "typecheck": "tsc",
    "sass": "sass --watch ./styles/index.scss:./app/styles/index.css --style compressed",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --quiet",
    "prettier": "prettier --write \"app/**/*.{ts,tsx}\""
  },
  "dependencies": {
    "@emailjs/browser": "^4.3.3",
    "@reduxjs/toolkit": "^2.2.1",
    "@remix-run/css-bundle": "^2.7.2",
    "@remix-run/node": "^2.7.2",
    "@remix-run/react": "^2.7.2",
    "@remix-run/vercel": "^1.19.3",
    "@vercel/analytics": "^1.2.2",
    "@vercel/node": "^2.10.3",
    "@vercel/speed-insights": "^1.0.10",
    "framer-motion": "^11.0.8",
    "isbot": "^5.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-redux": "^9.1.0",
    "react-responsive-carousel": "^3.2.23",
    "react-toastify": "^10.0.5"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.7.2",
    "@remix-run/eslint-config": "^2.7.2",
    "@remix-run/serve": "^2.7.2",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@types/react": "^18.2.60",
    "@types/react-dom": "^18.2.19",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "pretty-quick": "^4.0.0",
    "sass": "^1.71.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=v18.0.0"
  }
}
